# SimWidget Agent Configuration
# Copy this to .env and fill in your values

# Anthropic API Key (required)
# Get one at: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-xxxxx

# Agent server port (optional, default: 8585)
AGENT_PORT=8585

# SSL Configuration (optional)
# Run generate-certs.bat to create self-signed certificates
SSL_ENABLED=false
AGENT_SSL_PORT=8586
