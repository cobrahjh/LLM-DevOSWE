<service>
	<id>simwidgetmastero.exe</id>
	<name>SimWidget Master O</name>
	<description>SimWidget Engine Master Orchestrator - Service supervisor and watchdog</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>C:\DevOSWE\Admin\orchestrator\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\DevOSWE\Admin\orchestrator\orchestrator.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>SimWidget Master O wrapper</argument>
